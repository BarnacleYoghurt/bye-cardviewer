{
  "name": "bye-cardviewer/card",
  "apiVersion": 2,
  "version": "0.1.3",
  "title": "BYE Card",
  "editorScript": "file:../../build/index.js",
  "style": "file:../../build/index.css",
  "category": "bye-blocks",
  "icon": "interactive",
  "description": "Displays information on a card from BYE.",
  "attributes":
  {
    "expansion": {
      "type": "string"
    },
    "cardId": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "language": {
      "type": "string",
      "default": "en"
    },
    "cardOfTheDay": {
      "type": "boolean"
    },
    "fromUrlParams": {
      "type": "boolean"
    },
    "urlParamCardId": {
      "type": "string",
      "default": "cardId"
    },
    "urlParamVersion": {
      "type": "string",
      "default": "version"
    },
    "urlParamLanguage": {
      "type": "string",
      "default": "language"
    },
    "selectableCard": {
      "type": "boolean"
    },
    "selectableVersion": {
      "type": "boolean"
    },
    "selectableLanguage": {
      "type": "boolean"
    }
  }
}