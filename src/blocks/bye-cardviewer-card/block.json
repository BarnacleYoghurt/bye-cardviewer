{
  "name": "bye-cardviewer/card",
  "apiVersion": 2,
  "version": "0.1.2",
  "title": "BYE Card",
  "editorScript": "file:./index.js",
  "style": "file:./styles.css",
  "category": "bye-blocks",
  "icon": "interactive",
  "description": "Displays information on a card from BYE.",
  "attributes":
  {
    "expansion": {
      "type": "string"
    },
    "cardId": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "language": {
      "type": "string",
      "default": "en"
    },
    "cardOfTheDay": {
      "type": "boolean"
    },
    "fromUrlParams": {
      "type": "boolean"
    },
    "urlParamCardId": {
      "type": "string",
      "default": "cardId"
    },
    "urlParamVersion": {
      "type": "string",
      "default": "version"
    },
    "selectableCard": {
      "type": "boolean"
    },
    "selectableVersion": {
      "type": "boolean"
    },
    "selectableLanguage": {
      "type": "boolean"
    }
  }
}